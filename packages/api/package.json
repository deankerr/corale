{
  "name": "@corale/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "convex dev",
    "build": "pnpx convex deploy --cmd-url-env-var-name NEXT_PUBLIC_CONVEX_URL --cmd 'pnpm -w build:esuite' --preview-run init",
    "convex-init": "pnpx convex run init",
    "convex-deploy": "convex deploy",
    "typecheck": "tsc --noEmit",
    "generate-api": "mkdir -p build && convex function-spec > build/fn-spec.json && convex-helpers ts-api-spec --input-file build/fn-spec.json --output-file ${OUTPUT_FILE:-build/convex-api}"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.0.2",
    "@types/node": "^22.7.9",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.67",
    "@clerk/backend": "^1.15.1",
    "@fal-ai/client": "^1.0.2",
    "ai": "^3.4.18",
    "convex": "^1.17.0",
    "convex-ents": "^0.11.0",
    "convex-helpers": "^0.1.62",
    "deep-object-diff": "^1.1.9",
    "fast-content-type-parse": "^2.0.0",
    "itty-time": "^1.0.6",
    "ky": "^1.7.2",
    "nanoid": "^5.0.7",
    "remeda": "^2.16.0",
    "sharp": "^0.33.5",
    "svix": "^1.37.0",
    "ufo": "^1.5.4",
    "valibot": "^0.42.1",
    "zod": "^3.23.8"
  }
}
